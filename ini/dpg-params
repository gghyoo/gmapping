#ACES
graphmap_test -filename aces_publicb.log  -delta 0.1 -lstep 0.1 -lsigma 0.1 -linearUpdate 1 -angularUpdate 0.5 -llsamplestep 0.1 -llsamplerange 0.4  -maxrange 50 -maxUrange 50 -particles 200 -samplingLikelihoodGain 1 -likelihoodGain 3 -closureNeffThreshold 0.01 -minimumOverlappingCells 50 -likelihoodMapDelta 0.1 | gnuplot 

#INTEL
graphmap_test -filename intel.log  -delta 0.1 -lstep 0.1 -lsigma 0.1 -linearUpdate 0.25 -angularUpdate 0.1 -llsamplestep 0.1 -llsamplerange 0.4  -maxrange 50 -maxUrange 50 -particles 200 -samplingLikelihoodGain 1 -likelihoodGain 3 -closureNeffThreshold 0.01 -minimumOverlappingCells 20 -likelihoodMapDelta 0.1 -sightWindow 20| gnuplot 

#ISA
graphmap_test -filename isa_outdoor.log -delta 0.2 -lstep 0.1 -lsigma 0.1 -linearUpdate 1 -angularUpdate 0.1 -llsamplestep 0.1 -llsamplerange 0.4  -maxrange 50 -maxUrange 50 -particles 500 -samplingLikelihoodGain 1 -likelihoodGain 8 -closureNeffThreshold 0.01 -minimumOverlappingCells 50-likelihoodMapDelta 0.2 | gnuplot

#MIT
graphmap_test -filename MIT_Infinite_Corridor_2002_09_11_same_floor.log  -delta 0.1 -lstep 0.1 -lsigma 0.1 -linearUpdate 1 -angularUpdate 0.1 -llsamplestep 0.1 -llsamplerange 0.4  -maxrange 50 -maxUrange 50 -particles 200 -samplingLikelihoodGain 1 -likelihoodGain 2 -closureNeffThreshold 0.01 -minimumOverlappingCells 50 -likelihoodMapDelta 0.1 | gnuplot 

